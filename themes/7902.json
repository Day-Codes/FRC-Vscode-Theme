{
    "name": "7902 Theme",
    "type": "dark",
    "colors": {
        "editor.background": "#0B32B0", 
        "editor.foreground": "#8CECF8", 
        "editorCursor.foreground": "#3685E5", 
        "editor.lineHighlightBackground": "#1647BD", 
        "editor.selectionBackground": "#4CAEFF", 
        "editor.inactiveSelectionBackground": "#2D2D2D",
        "editorIndentGuide.background": "#2D2D2D",
        "editorIndentGuide.activeBackground": "#3D3D3D",
        "sideBar.background": "#0B32B0", 
        "sideBar.foreground": "#8CECF8", 
        "sideBar.border": "#3685E5", 
        "sideBarTitle.foreground": "#8CECF8",
        "sideBarSectionHeader.background": "#1647BD",
        "sideBarSectionHeader.foreground": "#8CECF8",
        "list.activeSelectionBackground": "#4CAEFF",
        "list.activeSelectionForeground": "#8CECF8",
        "list.inactiveSelectionBackground": "#2D2D2D",
        "list.inactiveSelectionForeground": "#8CECF8",
        "list.hoverBackground": "#3D3D3D",
        "list.hoverForeground": "#8CECF8",
        "list.focusBackground": "#4CAEFF",
        "list.focusForeground": "#8CECF8",
        "activityBar.background": "#0B32B0", 
        "activityBar.foreground": "#3685E5", 
        "activityBarBadge.background": "#3685E5", 
        "statusBar.background": "#0B32B0",
        "statusBar.foreground": "#8CECF8",
        "titleBar.activeBackground": "#0B32B0",
        "titleBar.activeForeground": "#8CECF8",
        "titleBar.border": "#3685E5",
        "tab.activeBorderTop": "#3685E5",
        "tab.unfocusedActiveBorderTop": "#215BCA",
        "editorGroupHeader.tabsBackground": "#0B32B0",
        "tab.activeBackground": "#0B32B0",
        "tab.inactiveBackground": "#1647BD",
        "tab.hoverBackground": "#1647BD",
        "tab.unfocusedActiveBackground": "#1647BD",
        "tab.unfocusedInactiveBackground": "#1647BD",
        "tab.activeForeground": "#FFFFFF",
        "tab.inactiveForeground": "#CCCCCC",
        "tab.hoverForeground": "#FFFFFF",
        "tab.border": "#3685E5",
        "tab.activeBorder": "#3685E5",
        "tab.inactiveBorder": "#215BCA"
    },
    "tokenColors": [
        {
            "scope": [
                "comment",
                "punctuation.definition.comment",
                "string.comment"
            ],
            "settings": {
                "foreground": "#7A7A7A"
            }
        },
        {
            "scope": [
                "keyword",
                "storage.type",
                "storage.modifier"
            ],
            "settings": {
                "foreground": "#3685E5" 
            }
        },
        {
            "scope": "constant",
            "settings": {
                "foreground": "#9B46B4"
            }
        },
        {
            "scope": "variable",
            "settings": {
                "foreground": "#8CECF8" 
            }
        },
        {
            "scope": "string",
            "settings": {
                "foreground": "#B135E6"
            }
        },
        {
            "scope": "support.function",
            "settings": {
                "foreground": "#3685E5"
            }
        },
        {
            "scope": "entity.name.function",
            "settings": {
                "foreground": "#9B46B4"
            }
        },
        {
            "scope": "entity.name.type",
            "settings": {
                "foreground": "#3685E5"
            }
        },
        {
            "scope": "keyword.operator",
            "settings": {
                "foreground": "#3685E5"
            }
        },
        {
            "scope": "punctuation.separator",
            "settings": {
                "foreground": "#8CECF8"
            }
        }
    ]
}
