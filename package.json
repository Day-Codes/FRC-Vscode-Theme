{
    "name": "FRC-Theme-Package",
    "displayName": "FRC Theme-Package",
    "description": "A Vscode Theme that is more FRC Related",
    "version": "0.1.0",
    "publisher": "Alfie-Devs",
    "engines": {
        "vscode": "^1.45.0"
    },
    "categories": [
        "Themes"
    ],
    "repository": {
        "type": "git",
        "url": "https://github.com/Day-Codes/FRC-Vscode-Theme"
    },
    "contributes": {
        "themes": [
            {
                "label": "WPLIB Theme",
                "uiTheme": "vs-dark",
                "path": "./themes/wplib.json"
            },
            {
                "label": "DAVE Theme",
                "uiTheme": "vs-dark",
                "path": "./themes/dave.json"
            },
            {
                "label": "2609 Theme",
                "uiTheme": "vs-dark",
                "path": "./themes/2609.json"
            },
            {
                "label": "WPLIB Light",
                "uiTheme": "vs-light",
                "path": "./themes/wplib-light.json"
            }, 
            { 
                "label": "7902 Theme Blue",
                "uiTheme": "vs-dark",
                "path": "./themes/7902.json"
            }, 
            {
                "label": "7902 Dark theme",
                "uiTheme": "vs-dark",
                "path": "./themes/7902-dark.json"
            }
        ]
    },
    "dependencies": {
        "vsce": "^2.15.0"
    }
}
